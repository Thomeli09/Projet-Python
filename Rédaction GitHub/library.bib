	file = {Limousin et al. - 2007 - Sorption isotherms A review on physical bases, mo.pdf:C\:\\Users\\Thommes Eliott\\Zotero\\storage\\2BEBGYZ6\\Limousin et al. - 2007 - Sorption isotherms A review on physical bases, mo.pdf:application/pdf},
