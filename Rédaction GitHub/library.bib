	pages = {7654--7665},
	file = {George et al. - 2007 - Elevated atmospheric CO2 concentration and tempera.pdf:C\:\\Users\\Thommes Eliott\\Zotero\\storage\\SGLY459B\\George et al. - 2007 - Elevated atmospheric CO2 concentration and tempera.pdf:application/pdf},
